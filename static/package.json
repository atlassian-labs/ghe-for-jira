{
  "name": "forge-ui-starter-typescript-experimental",
  "version": "0.0.0",
  "main": "./src/index.tsx",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "type": "commonjs",
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/node": "^12.20.19",
    "@types/react": "^16.14.13",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^5.3.2",
    "mocha": "^8.4.0",
    "ts-jest": "^26.4.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@atlaskit/button": "^16.0.0",
    "@atlaskit/code": "^14.1.3",
    "@atlaskit/css-reset": "^6.1.1",
    "@atlaskit/form": "^8.3.1",
    "@atlaskit/icon": "^21.8.1",
    "@atlaskit/page-header": "^10.1.2",
    "@atlaskit/spinner": "^15.0.11",
    "@atlaskit/textfield": "^5.1.1",
    "@atlaskit/theme": "^11.4.1",
    "@babel/register": "^7.15.3",
    "@forge/babel-plugin-transform-ui": "^1.0.0",
    "@forge/bridge": "0.1.0-next.3",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "react-sweet-state": "^2.5.2",
    "styled-components": "^5.3.0",
    "ts-loader": "^6.2.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/common/test-utils/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
